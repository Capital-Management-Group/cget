{
  "compilerOptions": {
    "declaration": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "outDir": "./dist",
    "removeComments": false,
    "lib": ["dom", "es2015"],
    "strictNullChecks": true,
    "target": "es5"
  },
  "include": ["./src/*.ts"]
}
