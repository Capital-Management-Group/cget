{
  "name": "@wikipathways/cget",
  "version": "0.1.2",
  "description": "Robust streaming parallel download manager with filesystem cache",
  "main": "dist/cget.js",
  "typings": "dist/cget.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "rm -rf node_modules; npm run install; npm run test",
    "test": "npm-run-all test:*",
    "test:jest": "jest",
    "test:clean": "rm -rf cache/",
    "test:original": "npm-run-all test:original:*",
    "test:original:build": "tsc -p test",
    "test:original:run": "node test/serve.js test/cache index.html"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "node",
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "((/src/.*\\.test\\.(ts|tsx|js))|(/test/spec/.*\\.(ts|tsx|js)))$"
  },
  "author": "Juha JÃ¤rvi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charto/cget.git"
  },
  "bugs": {
    "url": "https://github.com/charto/cget/issues"
  },
  "homepage": "https://github.com/charto/cget#readme",
  "keywords": [
    "cache",
    "caching",
    "request",
    "fetch",
    "parallel",
    "download",
    "manager",
    "http"
  ],
  "dependencies": {
    "bluebird": "^3.5.5",
    "cwait": "^1.1.2",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.119",
    "@types/node": "^7.10.2",
    "@types/request": "^2.48.2",
    "jest": "^24.8.0",
    "lodash": "^4.17.14",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  }
}
